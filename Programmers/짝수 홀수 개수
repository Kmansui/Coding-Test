class Solution {
    public int[] solution(int[] num_list) {
        int[] answer = new int[2];
        int cntodd = 0;
        int cnteven = 0;
        for(int i=0; i<num_list.length;i++){
            if(num_list[i]%2==1){
                cntodd+=1;
            }else{
                cnteven+=1;
            }
        }
            answer[0] = cnteven;
            answer[1] = cntodd;
        return answer;
    }
}
